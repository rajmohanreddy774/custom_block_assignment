{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/my-price-table",
  "version": "0.1.0",
  "title": "My Price Table",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "supports": {
    "html": false
  },
  "textdomain": "my-price-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "title": {
      "type": "array",
      "source": "text",
      "selector": "h3"
    },
    "subtitle": {
      "type": "array",
      "source": "text",
      "selector": "h5"
    },
    "bio": {
      "type": "array",
      "source": "text",
      "selector": "p"
    },
    "mediaID": {
      "type": "number"
    },
    "mediaURL": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "src"
    },
    "alignment": {
      "type": "string",
      "default": "center"
    },
    "facebookURL": {
      "type": "string"
    },
    "twitterURL": {
      "type": "string"
    },
    "instagramURL": {
      "type": "string"
    },
    "linkedURL": {
      "type": "string"
    },
    "emailAddress": {
      "type": "string"
    }
  }
}